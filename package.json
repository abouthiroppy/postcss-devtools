{
  "name": "postcss-devtools",
  "version": "1.0.0",
  "description": "Log execution time for each plugin in a PostCSS instance.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src",
    "prepublish": "de dist && BABEL_ENV=publish babel src --out-dir dist --ignore /__tests__/",
    "report": "nyc report --reporter=html",
    "test": "npm run lint && nyc ava src/__tests__/"
  },
  "files": [
    "LICENSE-MIT",
    "dist"
  ],
  "keywords": [
    "css",
    "postcss",
    "postcss-plugin",
    "measure",
    "tool",
    "development"
  ],
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "coveralls": "^2.11.6",
    "del-cli": "^0.2.0",
    "eslint": "^1.10.3",
    "nyc": "^5.5.0"
  },
  "homepage": "https://github.com/postcss/postcss-devtools",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "postcss/postcss-devtools",
  "dependencies": {
    "chalk": "^1.1.1",
    "convert-hrtime": "^1.0.0",
    "is-promise": "^2.1.0",
    "postcss": "^5.0.14",
    "postcss-reporter": "^1.3.0",
    "pretty-hrtime": "^1.0.1",
    "text-table": "^0.2.0"
  },
  "ava": {
    "require": "babel-core/register"
  }
}
